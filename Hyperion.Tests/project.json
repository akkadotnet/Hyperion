        "System.Runtime.Serialization.Primitives": "4.3.0",
        "System.Runtime.Serialization.Formatters" : "4.3.0",
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-161205",
        "Newtonsoft.Json": "9.0.1",
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        }
      },
      "imports": [
        "netstandard1.6"
      ]
